 const employees = [
    {
      name: 'Atticus',
      employeeNumber: '2405',
      annualSalary: '47000',
      reviewRating: 3
    },
    {
      name: 'Jem',
      employeeNumber: '62347',
      annualSalary: '63500',
      reviewRating: 4
    },
    {
      name: 'Scout',
      employeeNumber: '6243',
      annualSalary: '74750',
      reviewRating: 5
    },
    {
      name: 'Robert',
      employeeNumber: '26835',
      annualSalary: '66000',
      reviewRating: 1
    },
    {
      name: 'Mayella',
      employeeNumber: '89068',
      annualSalary: '35000',
      reviewRating: 1
    }
  ];
  
  // INPUTS: A single employee object. Something like this:
      // {
      //   name: 'Atticus',
      //   employeeNumber: '2405',
      //   annualSalary: '47000',
      //   reviewRating: 3
      // }
  // OUTPUTS: A whole number, 0 through 13, that represents
  //          a given employee's bonus percentage.
  function calculateIndividualEmployeeBonus(employee) {  
    let bonusPercentage = 0;
  
    if (employee.reviewRating === 3) {
      bonusPercentage = 4;
    } else if (employee.reviewRating === 4) {
      bonusPercentage = 6;
    } else if (employee.reviewRating === 5) {
      bonusPercentage = 10;
    }
  
    // IF employeeNumber.length is 4 digits: add 5% to bonus
    if (employee.employeeNumber.length === 4) {
      bonusPercentage += 5;
    }
  
    // IF annualSalary > $65,000: subtract 1% from bonus
    if (Number(employee.annualSalary) > 65000) {
      bonusPercentage -= 1;
    }
  
    // Bonus must be >= 0% AND <= 13%
    if (bonusPercentage < 0) {
      bonusPercentage = 0;
    } else if (bonusPercentage > 13) {
      bonusPercentage = 13;
    }
  
    return bonusPercentage;
  }
  
  // INPUTS: A single employee object. Something like this:
      // {
      //   name: 'Atticus',
      //   employeeNumber: '2405',
      //   annualSalary: '47000',
      //   reviewRating: 3
      // }
  // OUTPUTS: A processed employee object. Something like this:
    // {
    //   name:
    //   bonusPercentage: 13
    //   totalCompensation: 3332.00
    //   totalBonus: 4500.21
    // }
  function processEmployee(employee) {
    let annualSalaryAsNumber = Number(employee.annualSalary);
    let bonusPercentage = calculateIndividualEmployeeBonus(employee);
  
    let totalBonus = annualSalaryAsNumber * (bonusPercentage / 100)
    let totalCompensation = totalBonus + annualSalaryAsNumber
    
    return {
      name: employee.name,
      bonusPercentage: bonusPercentage,
      totalCompensation: totalCompensation,
      totalBonus: totalBonus
    }
  }
  
  for (let oneEmployee of employees) {
    console.log(processEmployee(oneEmployee));
  }
  